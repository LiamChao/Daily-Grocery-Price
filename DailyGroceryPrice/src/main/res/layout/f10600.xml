<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundcolor2"
    android:orientation="vertical"
    android:padding="@dimen/pa_15"
    android:visibility="visible">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="370dp"
        android:layout_height="250dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/f10600_b001"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:background="@drawable/button03"
        android:text="@string/f10600_b001"
        android:textAllCaps="false"
        android:textColor="@color/Teal"
        android:textSize="24sp" />

    <Button
        android:id="@+id/f10600_b002"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_below="@+id/f10600_b001"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@drawable/button03"
        android:text="@string/f10600_b002"
        android:textAllCaps="false"
        android:textColor="@color/Teal"
        android:textSize="24sp" />

</RelativeLayout>