<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0C000000"
    android:padding="@dimen/mar_15">

    <Button
        android:id="@+id/f10702_btnOK"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_below="@+id/f10702_e002"
        android:layout_alignParentStart="true"
        android:layout_marginTop="100dp"
        android:background="@drawable/button04"
        android:text="@string/f10702_b001"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/f10702_t001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="0dp"
        android:text="@string/f10702_e001"
        android:textColor="@color/Teal"
        android:visibility="invisible" />

    <EditText
        android:id="@+id/f10702_e001"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="@dimen/mar_15"
        android:autofillHints=""
        android:backgroundTint="@android:color/background_light"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:gravity="center_vertical"
        android:hint="@string/f10702_e001"
        android:inputType="textWebPassword"
        android:textColorHighlight="@color/Teal"
        android:textSize="18sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/f10702_t002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f10702_e001"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        android:text="@string/f10702_e002"
        android:textColor="@color/Teal"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/f10702_t003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f10702_e002"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:text="@string/f10702_t003"
        android:textColor="@color/Red"
        android:visibility="invisible" />

    <EditText
        android:id="@+id/f10702_e002"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_below="@+id/f10702_e001"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="@dimen/mar_15"
        android:autofillHints=""
        android:backgroundTint="@android:color/background_light"
        android:ems="10"
        android:gravity="center_vertical"
        android:hint="@string/f10702_e002"
        android:inputType="textWebPassword"
        android:textColorHighlight="@color/Teal"
        android:textSize="18sp" />

</RelativeLayout>